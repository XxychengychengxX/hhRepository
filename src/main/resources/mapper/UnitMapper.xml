<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.hhrepository.mapper.UnitMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.project.hhrepository.entity.Unit">
        <id column="unit_id" property="unitId" />
        <result column="unit_name" property="unitName" />
        <result column="unit_desc" property="unitDesc" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        unit_id, unit_name, unit_desc
    </sql>

</mapper>
